pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
            sh 'ps -ef'
            }
        }
        stage('Test') {
            steps {
            sh 'cd /root/myproject/
	    sh 'make'
            }
        }
        stage('Deploy') {
            steps {
            sh 'ls -ltr'
            }
        }
    }
}

