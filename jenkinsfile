    agent any
        stages {
            stage('Build') {
                steps {
                     sh "ps -ef"
                }
            }
            stage('Test') {
                steps {
                    echo 'This is the Testing Stage'
                }
            }
            stage('Deploy') {
                steps {
                    echo 'This is the Deploy Stage'
                }
            }
        }
        }
    
